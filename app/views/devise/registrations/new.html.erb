<div id="content">
<h2 class="title"><%= page_title! 'Inscribirse' %></h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :nombre %><br />
  <%= f.text_field :nombre %></div>

  <div><%= f.label :apellido %><br />
  <%= f.text_field :apellido %></div>

  <div><%= f.label :telefono %><br />
  <%= f.text_field :telefono %></div>

  <div><%= f.label :altura %><br />
  <%= f.text_field :altura %></div>

  <div><%= f.label :peso %><br />
  <%= f.text_field :peso %></div>

  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password, "Contraseña" %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation, "Confirmación de contraseña" %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Inscribirse" %></div>
<% end %>

</div>