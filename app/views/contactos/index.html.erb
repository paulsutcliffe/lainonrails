<h1>Listing contactos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Email</th>
    <th>Telefono</th>
    <th>Direccion</th>
    <th>Distrito</th>
    <th>Ciudad</th>
    <th>Provincia</th>
    <th>Mensaje</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contactos.each do |contacto| %>
  <tr>
    <td><%= contacto.nombre %></td>
    <td><%= contacto.apellido %></td>
    <td><%= contacto.email %></td>
    <td><%= contacto.telefono %></td>
    <td><%= contacto.direccion %></td>
    <td><%= contacto.distrito %></td>
    <td><%= contacto.ciudad %></td>
    <td><%= contacto.provincia %></td>
    <td><%= contacto.mensaje %></td>
    <td><%= link_to 'Show', contacto %></td>
    <td><%= link_to 'Edit', edit_contacto_path(contacto) %></td>
    <td><%= link_to 'Destroy', contacto, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contacto', new_contacto_path %>
